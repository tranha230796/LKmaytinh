@model LKmaytinh.Models.NhaSX
@{
                /**/

                ViewBag.Title = "ThemNSX";
                Layout = "~/Views/LayOut/AdminLayout.cshtml";
}

<h2>Thêm Nhà Sản Xuất</h2>
@using (Html.BeginForm("ThemNSX", "Admin", FormMethod.Post, new { @enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.LabelFor(model => model.TenNSX, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.TenNSX)
                @Html.ValidationMessageFor(model => model.TenNSX)
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="THÊM" class="btn btn-default" />
        </div>
    </div>
   
}
<div>
    @Html.ActionLink("NSX", "NSX","Admin")
</div>


